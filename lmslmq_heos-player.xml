                            <parameter name="UID" state_variable="UID" parameter_data_type="character" isHttpBody="true"/>
                            <parameter parameter_data_type="character" isHttpBody="true">",["power","0"]]}</parameter>
                        </parameter_list>
                        <delay ms_delay="2000"/>
                    </command>
                </command_interface>
            </action>
        </universal_component_actions>
